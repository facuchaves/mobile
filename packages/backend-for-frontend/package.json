{
  "name": "backend-jobs",
  "version": "1.0.15",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "mv-coverage": "mv output/coverage/cobertura-coverage.xml output/coverage/backend-coverage.xml",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "yarn bm-local",
    "start-ts": "ts-node -r tsconfig-paths/register src/main.ts",
    "bm-local": "cross-env ENVFILE=.env.local.bm yarn start:dev",
    "bm-mobile-local": "cross-env ENVFILE=.env.local.bm NODE_ENV=mobile concurrently --handle-input \"wait-on dist/main.js && nodemon --config nodemon-mobile.json\" \"tsc -w -p tsconfig.build.json\"",
    "zj-local": "cross-env ENVFILE=.env.local.zj yarn start:dev",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "ENVFILE=test.env jest",
    "test:watch": "ENVFILE=test.env jest --watch",
    "test:cov": "ENVFILE=test.env jest --coverage",
    "test:debug": " node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "validate": "mkdir -p output/test && TEST_REPORT_PATH=output/test ENVFILE=test.env jest --coverage --detectOpenHandles && yarn run mv-coverage",
    "postinstall": "bin/postinstall"
  },
  "dependencies": {
    "@google-cloud/datastore": "3.1.1",
    "@google-cloud/logging-winston": "1.1.1",
    "@google-cloud/trace-agent": "3.6.0",
    "@nestjs-plus/common": "1.1.0",
    "@nestjs/common": "6.2.4",
    "@nestjs/core": "6.2.4",
    "@nestjs/jwt": "6.1.1",
    "@nestjs/microservices": "6.2.4",
    "@nestjs/passport": "6.1.0",
    "@nestjs/platform-express": "6.2.4",
    "@nestjs/swagger": "3.0.2",
    "@newrelic/native-metrics": "4.1.0",
    "cache-manager": "2.9.0",
    "cache-manager-redis-store": "1.5.0",
    "cookie-parser": "1.4.4",
    "cross-env": "5.2.0",
    "dateformat": "3.0.3",
    "dotenv": "8.0.0",
    "lodash": "4.17.11",
    "multer": "1.4.2",
    "newrelic": "5.6.3",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "query-string": "6.8.2",
    "path": "0.12.7",
    "redis": "2.8.0",
    "reflect-metadata": "0.1.13",
    "request-context": "2.0.0",
    "rimraf": "2.6.2",
    "rxjs": "6.5.2",
    "swagger-ui-express": "4.0.6",
    "util": "0.12.0",
    "uuid": "3.3.2",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@nestjs/testing": "6.2.4",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.0",
    "@types/jest": "23.3.14",
    "@types/newrelic": "4.11.0",
    "@types/node": "10.14.8",
    "@types/redis": "2.8.13",
    "@types/supertest": "2.0.7",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "concurrently": "4.1.0",
    "git-repo-info": "2.1.0",
    "gulp": "4.0.2",
    "gulp-babel": "6.1.3",
    "jest": "23.6.0",
    "jest-junit-reporter": "1.1.0",
    "jest-sonar-reporter": "2.0.0",
    "moment": "2.24.0",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "supertest": "3.4.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "wait-on": "3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node",
    "testResultsProcessor": "../testResultsProcessor",
    "coverageReporters": [
      "lcov",
      "cobertura"
    ],
    "coverageDirectory": "../output/coverage"
  }
}
